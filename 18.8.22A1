#include<stdio.h>
int main()
{
	int a[]={1,1,2,3,4,5};
	int len=sizeof(a)/sizeof(int);
	int i,x,cnt=0;
	for(i=0;i<len;i++)
	{
		x=a[i];
		if(x==a[i+1])
		cnt++;
	}
	if(cnt==0)
	printf("\nUnique");
	else
	printf("\nDuplicate");
    return 0;
}

